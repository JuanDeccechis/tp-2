[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/JuanDeccechis
[access "refs/for/refs/*"]
	read = group user/JuanDeccechis
	push = group user/JuanDeccechis
	label-Code-Review = -2..+2 group user/JuanDeccechis
	label-Verified = -1..+1 group user/JuanDeccechis
	rebase = group user/JuanDeccechis
	removeReviewer = group user/JuanDeccechis
	submit = group user/JuanDeccechis
[access "refs/heads/*"]
	read = group user/JuanDeccechis
	create = group user/JuanDeccechis
	push = +force group user/JuanDeccechis
	pushMerge = group user/JuanDeccechis
[access "refs/tags/*"]
	push = group user/JuanDeccechis
	push = +force group user/JuanDeccechis
